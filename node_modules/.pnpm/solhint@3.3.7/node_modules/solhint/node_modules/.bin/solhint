#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/takayukiono/Desktop/my-project/node_modules/.pnpm/solhint@3.3.7/node_modules/solhint/node_modules:/Users/takayukiono/Desktop/my-project/node_modules/.pnpm/solhint@3.3.7/node_modules:/Users/takayukiono/Desktop/my-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/takayukiono/Desktop/my-project/node_modules/.pnpm/solhint@3.3.7/node_modules/solhint/node_modules:/Users/takayukiono/Desktop/my-project/node_modules/.pnpm/solhint@3.3.7/node_modules:/Users/takayukiono/Desktop/my-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../solhint.js" "$@"
else
  exec node  "$basedir/../../solhint.js" "$@"
fi
